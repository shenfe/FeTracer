{
    "name": "fetracer",
    "description": "online analysis of frontend codes",
    "version": "1.0.0",
    "devtools_page": "devtools.html",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "<all_urls>",
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": [
            "util/object.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        },
        "default_title": "FeTracer",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png",
        "19": "images/icon19.png",
        "38": "images/icon38.png"
    },
    "manifest_version": 2
}
